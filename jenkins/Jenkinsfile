pipeline {
    agent any
   
    
    stages {
        stage('Checkout') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/aakashsehgal/FMU.git']]])
                sh "ls -lart ./*"
            }
        }     
        stage('Build') {
            steps {
                echo "Build Phase"
            }
        }
        stage('Test') {
            steps {
                echo "Test Phase"
            }
        }
        stage('Deliver') {
            steps {
                echo "Delivery Phase"
            }
        }
    }
}
